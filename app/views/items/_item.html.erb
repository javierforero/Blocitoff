<div class=<%= "item-#{item.id}" %>>
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-2"></div>
    <div class="col-md-2">
      <%= item.name %><br>
      <small><%= "created #{time_ago_in_words(item.created_at)} ago"%></small>
    </div>
    <div class="col-md-2"><%= link_to "",[item.user, item], method: :delete,remote: true, class: "glyphicon glyphicon-ok" %><br></div>
    <div class="col-md-2"></div>
    <div class="col-md-2"></div>
  </div>
</div>
